{
  "manifest_version": 2,
  "name": "Notist",
  "description": "Web Annotation",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "activeTab", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Create annotation",
    "default_popup": "index.html"
  }
}
